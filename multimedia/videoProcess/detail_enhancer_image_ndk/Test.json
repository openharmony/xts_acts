{
    "description": "Config for avcodec ndk test cases",
    "driver": {
        "module-name": "ActsDetailEnhancerImageNdkInnerUnitTest",
        "native-test-timeout": "300000",
        "native-test-device-path": "/data/local/tmp",
        "runtime-hint": "1s",
        "type": "CppTest"
    },
    "kits": [
        {
            "type": "PushKit",
            "pre-push" : [
                "mount -o rw,remount /",
                "mkdir -p /data/test/media",
                "mkdir -p /data/local/tmp/"
            ],
            "push": [
                "ActsDetailEnhancerImageNdkInnerUnitTest->/data/local/tmp/ActsDetailEnhancerImageNdkInnerUnitTest"
            ]
        },
        {
            "type": "ShellKit",
            "run-command": [
                "hilog -Q pidoff",
                "chmod 777 -R /data/local/tmp",
                "restorecon /data/test/media"
            ]
        }
    ]
}