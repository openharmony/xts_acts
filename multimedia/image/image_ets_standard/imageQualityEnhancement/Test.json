{
  "description": "Configuration for hjunit demo Tests",
  "driver": {
    "type": "OHJSUnitTest",
    "test-timeout": "300000",
    "bundle-name": "ohos.acts.multimedia.image.imagequalityenhancement",
    "module-name": "entry",
    "shell-timeout": "300000",
    "testcase-timeout": "180000"
  },
  "kits": [
    {
      "test-file-name": [
        "ActsImageQualityEnhancementTest.hap"
      ],
      "type": "AppInstallKit",
      "cleanup-apps": true
    },
    {
      "type": "ShellKit",
      "run-command": [
        "mkdir -p /data/app/el2/100/base/ohos.acts.multimedia.image.imagequalityenhancement/haps/entry/files/"
      ]
    },
    {
      "type": "PushKit",
      "pre-push": [],
      "push": [
        "./resource/image/test.jpg ->/data/app/el2/100/base/ohos.acts.multimedia.image.imagequalityenhancement/haps/entry/files",
        "./resource/image/test.bmp ->/data/app/el2/100/base/ohos.acts.multimedia.image.imagequalityenhancement/haps/entry/files",
        "./resource/image/test.png ->/data/app/el2/100/base/ohos.acts.multimedia.image.imagequalityenhancement/haps/entry/files"
      ]
    },
    {
      "type": "ShellKit",
      "run-command": [
        "chmod -R 777 /data/app/el2/100/base/ohos.acts.multimedia.image.imagequalityenhancement/haps/entry/files/*"
      ]
    }
  ]
}
