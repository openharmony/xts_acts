{
    "description": "Configuration for hjunit demo Tests",
    "driver": {
      "type": "OHJSUnitTest",
      "test-timeout": "160000",
      "shell-timeout": "160000",
      "bundle-name": "ohos.acts.usb.usb.function",
      "package-name": "ohos.acts.usb.usb.function"
    },
    "kits": [
        {
            "test-file-name": [
              "ActsUsbJSApiTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },{
            "type": "ShellKit",
            "run-command": [
                "power-shell wakeup",
                "power-shell setmode 602",
                "uitest dumpLayout"
            ]
        }
    ]
}