{
    "description": "Configuration for ActsHiAppEvent Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "180000",
        "bundle-name": "com.hiappevent.pamtest",
        "module-name": "entry_test",
        "shell-timeout": "600000",
        "testcase-timeout": 60000
    },
    "kits": [
        {
            "test-file-name": [
                "ActsHiAppEventPamTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
    }, {
            "type": "ShellKit",
            "run-command": [
                "power-shell wakeup",
                "power-shell setmode 602"
            ]
        }
    ]
}