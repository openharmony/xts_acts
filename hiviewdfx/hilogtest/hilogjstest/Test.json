{
    "description": "Configuration for hjunit demo Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "270000",
        "shell-timeout": "60000",
        "bundle-name": "ohos.acts.hiviewdfx.hilog.function",
        "package-name": "ohos.acts.hiviewdfx.hilog.function"
    },
    "kits": [
        {
            "test-file-name": [
                "ActsHilogJsTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
            "type": "ShellKit",
            "run-command": [
                "param get hilog.flowctrl.proc.on false"
            ],
            "teardown-command": [
                "param get hilog.flowctrl.proc.on true"
            ]
        }
    ]
}