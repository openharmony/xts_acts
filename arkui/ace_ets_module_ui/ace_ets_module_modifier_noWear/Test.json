{
    "description": "Configuration for hjunit demo Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "1200000",
	    "bundle-name": "com.open.harmony.modifiernowear",
	    "module-name": "entry_test",
	    "shell-timeout": "1200000",
	    "testcase-timeout": "180000"
    },
    "kits": [
        {
            "test-file-name": [
                "ActsAceEtsModuleModifierNoWearTest.hap",
                "ActsAceEtsModuleModifierNoWearTestMain.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
            "type": "ShellKit",
            "run-command": [
                "power-shell wakeup",
                "power-shell timeout -o 864000000"
            ]
        }
    ]
}
