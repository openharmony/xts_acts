@Entry
@Component
struct FontSettingOptions1 {
  @State text1: string = 'This is test text1'
  @State text2: string = 'This is test text2'

  build() {
    Column() {
      Text(this.text1)
        .key('FontTestText_01')
        .fontSize(20)
        .font({weight:FontWeight.Bold},{enableVariableFontWeight:true})
      Text(this.text2)
        .key('FontTestText_02')
        .fontSize(20)
        .fontWeight(FontWeight.Bold,{enableVariableFontWeight:true})
    }
  }
}