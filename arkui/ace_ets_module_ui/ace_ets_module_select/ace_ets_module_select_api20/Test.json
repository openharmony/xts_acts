{
    "description": "Configuration for hjunit demo Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "180000",
        "bundle-name": "com.arkui.ace.select.api20",
        "module-name": "entry_test",
        "shell-timeout": "600000",
        "testcase-timeout": "180000"
    },
    "kits": [
        {
            "test-file-name": [
                "ActsAceEtsModuleSelectApi20Test.hap",
                "ActsAceEtsModuleSelectApi20TestMain.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
            "type": "ShellKit",
            "run-command": [
                "power-shell wakeup",
                "power-shell timeout -o 864000000"
            ]
        }
    ]
}