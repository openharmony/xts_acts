{
    "description": "Configuration for hjunit demo Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "180000",
        "bundle-name": "com.arkui.ace.layout.nowear.api11",
        "module-name": "entry_test",
        "shell-timeout": "600000",
        "testcase-timeout": "180000"
    },
    "kits": [{
        "test-file-name": [
            "ActsAceEtsModuleLayoutNowearApi11Test.hap",
            "ActsAceEtsModuleLayoutNowearApi11TestMain.hap"
        ],
        "type": "AppInstallKit",
        "cleanup-apps": true
        },
        {
            "type": "ShellKit",
            "run-command": [
                "power-shell wakeup",
                "power-shell timeout -o 864000000"
            ]
        }
    ]
}