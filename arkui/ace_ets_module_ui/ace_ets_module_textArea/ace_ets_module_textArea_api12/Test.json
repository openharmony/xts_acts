{
    "description": "Configuration for for hjunit demo Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "10800000",
        "bundle-name": "com.acts.arkui.component.common.attrs.test",
        "module-name": "entry_test",
        "shell-timeout": "10800000",
        "testcase-timeout": 300000
    },
    "kits": [
        {
            "test-file-name": [
                "ActsAceEtsModuleTextAreaApi12Test.hap",
                "ActsAceEtsModuleTextAreaApi12TestMain.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
            "type": "ShellKit",
            "run-command": [
                "power-shell wakeup",
                "uinput -T -m 300 2700 280 200",
                "power-shell setmode 602",
                "power-shell timeout -o 864000000"
            ]
        }
    ]
}