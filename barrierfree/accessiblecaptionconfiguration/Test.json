{
	"description":"Configuration for hjunit demo Tests",
	"driver":{
		"type":"OHJSUnitTest",
		"test-timeout":"180000",
		"shell-timeout":"180000",
		"bundle-name":"com.example.accessiblecaptionconfiguration",
		"package-name":"com.example.accessiblecaptionconfiguration_test"
	},
	"kits":[
		{
			"test-file-name":[
				"ActsAccessibleCaptionConfigurationTest.hap"
			],
			"type":"AppInstallKit",
			"cleanup-apps":true
		},
		{
			"type":"ShellKit",
			"run-command":[
				"power-shell wakeup",
				"power-shell setmode 602"
			]
		}
	]
}