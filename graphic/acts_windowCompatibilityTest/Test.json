{
    "description": "Configuration for hjunit demo Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "180000",
        "bundle-name": "com.example.myapplication",
        "module-name": "phone",
        "shell-timeout": "600000",
        "testcase-timeout": "70000"
    },
    "kits": [
        {
            "type": "ShellKit",
            "run-command": [
                "wait 5",
                "power-shell wakeup",
                "uinput -T -m 300 2700 280 200",
                "wait 5",
                "uinput -T -m 300 2700 280 200",
                "power-shell setmode 602"
            ]
        },
        {
            "test-file-name": [
                "ActsWindowCompatibilityTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        }
    ]
}