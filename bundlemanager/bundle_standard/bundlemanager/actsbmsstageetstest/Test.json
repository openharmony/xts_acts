{
    "description": "Configuration for hjunit demo Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "180000",
        "bundle-name": "ohos.acts.bundle.stage.test",
        "module-name": "entry",
        "shell-timeout": "600000",
        "testcase-timeout": 70000
    },
    "kits": [
        {
            "test-file-name": [
                "ActsBmsStageEtsTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
            "type": "ShellKit",
            "teardown-command":[
                "bm uninstall -n ohos.acts.bundle.stage.test"
            ]
        }
    ] 
}

