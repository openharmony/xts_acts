import("//test/xts/tools/build/suite.gni")

config("nnrt_config") {
  include_dirs = [
    "../common",
  ]
  cflags = [ "-Wno-error" ]
  cflags_cc = [ "-fexceptions" ]
}

ohos_moduletest_suite("ActsAiNnrtFunctionV2_0Test") {
  module_out_path = "acts/nnrt"
  sources = [
    "../common/mock_idevice.cpp",
    "../common/nnrt_utils.cpp",
    "src/CompileTest.cpp",
    "src/DeviceTest.cpp",
    "src/ExecutorTest.cpp",
    "src/MemoryTest.cpp",
    "src/ModelTest.cpp",
  ]

  configs = [ ":nnrt_config" ]

  external_deps = [
    "c_utils:utils",
    "drivers_interface_nnrt:libnnrt_proxy_2.0",
    "googletest:gmock_main",
    "googletest:gtest_main",
    "hdf_core:libhdf_utils",
    "hdf_core:libhdi",
    "hilog:libhilog",
    "hitrace:libhitracechain",
    "ipc:ipc_single",
    "mindspore:mindir_lib",
    "neural_network_runtime:libneural_network_core",
    "neural_network_runtime:libneural_network_runtime",
  ]

  subsystem_name = "ai"
  part_name = "neural_network_runtime"
}
