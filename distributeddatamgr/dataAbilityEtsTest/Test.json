{
    "description": "Configuration for hjunit demo Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "60000",
        "shell-timeout": "60000",
        "bundle-name": "ohos.acts.dataSharejstest",
        "module-name": "dataAbility"
    },
    "kits": [
        {
            "test-file-name": [
                "ActsDataAbilityTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
		{
			"type":"ShellKit",
			"run-command":[
				"power-shell wakeup",
				"uinput -T -g 100 100 500 500",
				"power-shell setmode 602"
			]
		}
    ]
}