{
    "description": "Configuration for hjunit demo Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "180000",
        "bundle-name": "com.example.apisystest",
        "module-name": "entry_test",
        "shell-timeout": "300000",
        "testcase-timeout": 20000
    },
    "kits": [
        {
            "test-file-name": [
                "ActsFAhapTest.hap",
                "ActsApiCoverhapMain2.hap",
                "FaSupplement.hap",
                "StageSupplement.hap",
                "FormModule.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },{
          "type": "ShellKit",
          "run-command": [
              "uitest dumpLayout"
          ]
      }
    ]
}