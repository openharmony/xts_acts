{
    "description": "Configuration for debuggable demo Tests",
    "driver": {
      "type": "OHJSUnitTest",
      "test-timeout": "180000",
      "bundle-name": "com.example.contextdirnormaltest",
      "module-name": "entry",
      "shell-timeout": "600000",
      "testcase-timeout": 15000
    },
    "kits": [
      {
        "test-file-name": [
          "ActsContextDirNormalTest.hap",
          "ActsContextDirNormalFeature.hap"
        ],
        "type": "AppInstallKit",
        "cleanup-apps": true
      },
      {
        "type": "ShellKit",
        "run-command": [
          "power-shell wakeup",
          "power-shell setmode 602",
          "uinput -T -m 300 2700 280 200"
        ]
      }
    ]
  }