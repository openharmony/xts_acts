{
    "description": "Configuration for hjunit Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "600000",
        "bundle-name": "com.acts.childprocessmanagertest",
        "module-name": "entry",
        "shell-timeout": "30000",
        "testcase-timeout": 60000
    },
    "kits": [
        {
            "test-file-name": [
                "ActsChildProcessManagerTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
            "type": "ShellKit",
            "run-command": [
                "power-shell wakeup"
            ]
        }
    ]
}