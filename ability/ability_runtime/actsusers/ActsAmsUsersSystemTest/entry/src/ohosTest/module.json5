{
  "module": {
    "name": "phone",
    "type": "feature",
    "srcEntry": "./ets/Application/AbilityStage.ts",
    "description": "$string:phone_entry_dsc",
    "mainElement": "MainAbility",
    "deviceTypes": [],
    "deliveryWithInstall": true,
    "installationFree": false,
    "pages": "$profile:test_pages",
    "abilities": [
      {
        "name": "com.acts.actsinterfacemultiuserstest.MainAbility",
        "srcEntry": "./ets/MainAbility/MainAbility.ts",
        "description": "$string:phone_entry_main",
        "icon": "$media:icon",
        "label": "$string:entry_label",
        "visible": true,
        "orientation": "portrait",
        "skills": [
          {
            "actions": [
              "action.system.home"
            ],
            "entities": [
              "entity.system.home"
            ]
          }
        ],
        "startWindowIcon": "$media:icon",
        "startWindowBackground": "$color:start_window_background"
      },
      {
        "name": "com.acts.actsinterfacemultiuserstest.MainAbility2",
        "srcEntry": "./ets/MainAbility2/MainAbility2.ts",
        "description": "$string:phone_entry_main",
        "icon": "$media:icon",
        "label": "$string:entry_label",
        "visible": true,
        "orientation": "portrait",
        "startWindowIcon": "$media:icon",
        "startWindowBackground": "$color:start_window_background"
      },
      {
        "name": "com.acts.actsinterfacemultiuserstest.MainAbility3",
        "srcEntry": "./ets/MainAbility3/MainAbility3.ts",
        "description": "$string:phone_entry_main",
        "icon": "$media:icon",
        "label": "$string:entry_label",
        "visible": true,
        "orientation": "portrait",
        "startWindowIcon": "$media:icon",
        "startWindowBackground": "$color:start_window_background"
      }
    ],
    "requestPermissions": [
      {
        "name": "ohos.permission.CLEAN_BACKGROUND_PROCESSES"
      },
      {
        "name": "ohos.permission.GET_BUNDLE_INFO"
      },
      {
        "name": "ohos.permission.GET_BUNDLE_INFO_PRIVILEGED"
      },
      {
        "name": "ohos.permission.GET_RUNNING_INFO"
      },
      {
        "name": "ohos.permission.START_ABILITIES_FROM_BACKGROUND"
      }
    ],
    "definePermissions": [
      {
        "grantMode": "system_grant",
        "name": "ohos.permission.MANAGE_LOCAL_ACCOUNTS"
      },
      {
        "grantMode": "system_grant",
        "name": "ohos.permission.INTERACT_ACROSS_LOCAL_ACCOUNTS_EXTENSION"
      }
    ]
  }
}