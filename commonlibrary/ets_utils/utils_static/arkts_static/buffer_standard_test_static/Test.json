{
    "description": "buffer test",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "800000",
        "shell-timeout": "800000",
        "bundle-name": "com.buffer.standard.static",
        "package-name": "com.buffer.standard.static"
    },
    "kits": [
        {
            "test-file-name": [
                "ActBufferStatic.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },{
            "type": "ShellKit",
            "run-command": [
                "power-shell wakeup",
                "power-shell setmode 602"
            ]
        }
    ]
}