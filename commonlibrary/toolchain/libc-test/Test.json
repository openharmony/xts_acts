                "rm -rf /data/local/tmp/libc-test/poll_sup",
                "rm -rf /data/local/tmp/libc-test/ipc_msg",
                "rm -rf /data/local/tmp/libc-test/ipc_sem",
                "rm -rf /data/local/tmp/libc-test/ipc_shm"
            ],
            "cleanup-apps": true,
            "teardown-push": [
                "rm -rf /data/local/tmp/zh_CN",
