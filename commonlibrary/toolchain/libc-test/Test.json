                "rm -rf /data/local/tmp/libc-test/ftok",
                "rm -rf /data/local/tmp/libc-test/lchown",
                "rm -rf /data/local/tmp/libc-test/syslog",
                "rm -rf /data/local/tmp/libc-test/vsyslog",
                "rm -rf /data/local/tmp/libc-test/dlns_dlopen_test ",
