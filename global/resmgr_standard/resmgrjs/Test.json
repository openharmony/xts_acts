{
    "description": "Configuration for resmgrjs Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "300000",
        "testcase-timeout": "300000",
        "shell-timeout": "60000",
        "bundle-name": "ohos.resmgr.test",
        "package-name": "ohos.resmgr.test"
    },
    "kits": [
        {
            "test-file-name": [
                "ActsResMgrJsTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
            "type": "ShellKit",
            "run-command": [
                "power-shell wakeup",
                "power-shell setmode 602"
            ]
        }
    ]
}