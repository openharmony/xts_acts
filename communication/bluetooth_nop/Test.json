{
    "description": "Configuration for hjunit demo Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "180000",
        "shell-timeout": "180000",
        "bundle-name": "ohos.acts.blutooth.nop.function",
        "module-name": "entry_test",
		"testcase-timeout": "20000"
    },
    "kits": [
        {
            "test-file-name": [
                "ActsBlutoothNopTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },{
            "type": "ShellKit",
            "run-command": [
                "power-shell wakeup",
                "uinput -T -m 300 2700 280 200",
                "power-shell timeout -o 864000000"
            ]
        }
    ]
}